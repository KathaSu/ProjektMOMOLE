<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="495dp"
    android:orientation="vertical"
    tools:context="com.momole.de.projektmomole.FAQActivity"
    tools:layout_editor_absoluteX="8dp"
    tools:layout_editor_absoluteY="8dp">

    <Button
        android:id="@+id/button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Wie kann ihnen diese App helfen?"
        tools:layout_editor_absoluteX="58dp"
        tools:layout_editor_absoluteY="30dp"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/button2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Was ist eine Lebensmittelunverträglichkeit?"
        tools:layout_editor_absoluteX="10dp"
        tools:layout_editor_absoluteY="93dp"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/button3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Wann sollte man einen Arzt aufsuchen?"
        tools:layout_editor_absoluteX="28dp"
        tools:layout_editor_absoluteY="157dp"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/button4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Welche Lebensmittel und Beschwerden lassen sich dokumentieren?"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="220dp"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/button5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Woher weiß ich, welches Allergen in meinen Lebensmitel enthalten ist?"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="290dp"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/button6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Was bedeuten die unterschiedlichen Symbole?"
        tools:layout_editor_absoluteX="7dp"
        tools:layout_editor_absoluteY="368dp"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/button7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Was ist mit Medikamenten und Umwelteinflüssen?"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="433dp"
        tools:ignore="HardcodedText" />

</LinearLayout>
